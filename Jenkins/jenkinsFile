node {
  stage('Git-Checkout') {
   git branch:'DevTest', url:'https://github.com/NiharSibu/Learning_Git.git'
  }
    
 def project_path="HotelManagement"
 
 dir(project_path) {
    
  stage('Maven-Clean') {
   sh label: '', script: 'mvn clean'
  }
    
 stage('Maven-Compile') {
   sh label: '', script: 'mvn compile'
  }
  
   stage('Maven-Test') {
   sh label: '', script: 'mvn test'
  }
  
   stage('Maven-Package') {
   sh label: '', script: 'mvn package'
  }
 }    
}
